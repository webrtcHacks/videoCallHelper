{
  "name": "Video Call Helper",
  "action": {
    "default_icon": {
    },
    "default_title": "Video Call Helper"
  },
  "manifest_version": 3,
  "version": "0.0.1",
  "description": "Video Call Helper",
  "permissions": [
    "activeTab",
    "tabs",
    "scripting",
    "storage",
    "unlimitedStorage"
  ],
  "background": {
    "service_worker": "scripts/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "exclude_matches": [],
      "all_frames": false,
      "match_about_blank": true,
      "run_at": "document_start",
      "js": [
        "scripts/content.js"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; style-src 'self' 'unsafe-inline'",
    "isolated_world": "script-src 'self' 'unsafe-inline'"
  },
  "sandbox": {
    "pages": [
      "pages/framingAnalysis.html"
    ]
  },
  "options_page": "pages/options.html",
  "web_accessible_resources": [
    {
      "resources": [
        "pages/*.html",
        "scripts/*.js",
        "styles/*.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
